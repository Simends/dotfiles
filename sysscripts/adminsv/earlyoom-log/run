#!/bin/execlineb -P
ifelse { test -w /var/log } {
	foreground { install -d -o s6log -g s6log /var/log/earlyoom }
	s6-setuidgid s6log exec -c s6-log -d3 -b -- n3 s2000000 T /var/log/earlyoom
}
foreground { install -d -o s6log -g s6log /run/log/earlyoom }
s6-setuidgid s6log exec -c s6-log -d3 -b -- n3 s2000000 T /run/log/earlyoom
